{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Oszlopdiagram az egyes foglalkozások megfigyeléseinek számával.",
  "data": {
    "url": "https://raw.githubusercontent.com/korosigreta/korosigreta.github.io/main/Mental%20Health%20Dataset.csv",
    "format": {"type": "csv"}
  },
  "width": 1000,
  "mark": {"type": "bar", "width": 100},
  "encoding": {
    "x": {
      "field": "Occupation",
      "type": "nominal",
      "axis": {"labelAngle": 0},
      "title": "Foglalkozatottság"
    },
    "y": {
      "aggregate": "count",
      "field": "*",
      "type": "quantitative",
      "title": "Megfigyelések száma"
    },
    "tooltip": [
      {"field": "Occupation", "type": "nominal", "title": "Occupation"},
      {
        "aggregate": "count",
        "field": "*",
        "type": "quantitative",
        "title": "Number of Observations"
      }
    ]
  },
  "config": {}
}